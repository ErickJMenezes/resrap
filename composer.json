{
  "name": "erickjmenezes/resrap",
  "description": "parser utils",
  "type": "library",
  "license": "mit",
  "bin": ["bin/resrap"],
  "autoload": {
    "psr-4": {
      "Resrap\\Component\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Resrap\\Examples\\": "examples/",
      "Resrap\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "ErickJMenezes",
      "email": "erickmenezes.dev@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "pestphp/pest": "^4.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
